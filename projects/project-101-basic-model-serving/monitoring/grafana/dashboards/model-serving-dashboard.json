{
  "dashboard": {
    "title": "Model Serving Dashboard",
    "tags": ["model-serving", "ml", "resnet50"],
    "timezone": "browser",
    "schemaVersion": 16,
    "version": 1,
    "refresh": "10s",
    "panels": [
      {
        "id": 1,
        "title": "API Request Rate",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "rate(http_requests_total{job=\"model-serving-api\"}[5m])",
            "legendFormat": "{{method}} {{handler}}"
          }
        ],
        "yaxes": [
          {"format": "reqps", "label": "Requests/sec"},
          {"format": "short"}
        ]
      },
      {
        "id": 2,
        "title": "API Latency (p95, p99)",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket{job=\"model-serving-api\"}[5m]))",
            "legendFormat": "p95"
          },
          {
            "expr": "histogram_quantile(0.99, rate(http_request_duration_seconds_bucket{job=\"model-serving-api\"}[5m]))",
            "legendFormat": "p99"
          }
        ],
        "yaxes": [
          {"format": "s", "label": "Latency"},
          {"format": "short"}
        ]
      },
      {
        "id": 3,
        "title": "HTTP Status Codes",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
        "targets": [
          {
            "expr": "rate(http_requests_total{job=\"model-serving-api\",status=\"200\"}[5m])",
            "legendFormat": "200 OK"
          },
          {
            "expr": "rate(http_requests_total{job=\"model-serving-api\",status=~\"4..\"}[5m])",
            "legendFormat": "4xx Client Error"
          },
          {
            "expr": "rate(http_requests_total{job=\"model-serving-api\",status=~\"5..\"}[5m])",
            "legendFormat": "5xx Server Error"
          }
        ],
        "yaxes": [
          {"format": "reqps", "label": "Requests/sec"},
          {"format": "short"}
        ]
      },
      {
        "id": 4,
        "title": "CPU Usage",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
        "targets": [
          {
            "expr": "rate(container_cpu_usage_seconds_total{pod=~\"model-serving-.*\"}[5m]) * 100",
            "legendFormat": "{{pod}}"
          }
        ],
        "yaxes": [
          {"format": "percent", "label": "CPU Usage"},
          {"format": "short"}
        ]
      },
      {
        "id": 5,
        "title": "Memory Usage",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16},
        "targets": [
          {
            "expr": "container_memory_usage_bytes{pod=~\"model-serving-.*\"} / 1024 / 1024 / 1024",
            "legendFormat": "{{pod}}"
          }
        ],
        "yaxes": [
          {"format": "decgbytes", "label": "Memory"},
          {"format": "short"}
        ]
      },
      {
        "id": 6,
        "title": "Active Pods",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 12, "y": 16},
        "targets": [
          {
            "expr": "count(kube_pod_status_phase{pod=~\"model-serving-.*\",phase=\"Running\"})"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area"
        }
      },
      {
        "id": 7,
        "title": "Total Requests (24h)",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 18, "y": 16},
        "targets": [
          {
            "expr": "sum(increase(http_requests_total{job=\"model-serving-api\"}[24h]))"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area"
        }
      },
      {
        "id": 8,
        "title": "Prediction Count",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 20},
        "targets": [
          {
            "expr": "rate(http_requests_total{job=\"model-serving-api\",handler=\"/predict\"}[5m])",
            "legendFormat": "Predictions/sec"
          }
        ],
        "yaxes": [
          {"format": "reqps", "label": "Predictions/sec"},
          {"format": "short"}
        ]
      },
      {
        "id": 9,
        "title": "Error Rate",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 20},
        "targets": [
          {
            "expr": "sum(rate(http_requests_total{job=\"model-serving-api\",status=~\"5..\"}[5m])) / sum(rate(http_requests_total{job=\"model-serving-api\"}[5m])) * 100",
            "legendFormat": "Error Rate %"
          }
        ],
        "yaxes": [
          {"format": "percent", "label": "Error Rate"},
          {"format": "short"}
        ],
        "alert": {
          "conditions": [
            {
              "evaluator": {"params": [5], "type": "gt"},
              "operator": {"type": "and"},
              "query": {"params": ["A", "5m", "now"]},
              "reducer": {"params": [], "type": "avg"},
              "type": "query"
            }
          ],
          "executionErrorState": "alerting",
          "frequency": "60s",
          "handler": 1,
          "name": "High Error Rate",
          "noDataState": "no_data",
          "notifications": []
        }
      }
    ]
  }
}
